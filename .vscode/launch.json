{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Jupyter: Debug Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": ["--allow-debugging"]
        }
    ]
}
